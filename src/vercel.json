{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "react",
  "functions": {},
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frontend-Only",
          "value": "true"
        }
      ]
    }
  ]
}